{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/training/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/api/training/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "SwaggerKey": "trainingSwagger"
    },
    {
      "DownstreamPathTemplate": "/api/subscription/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5002
        }
      ],
      "UpstreamPathTemplate": "/api/subscription/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "SwaggerKey": "subscriptionSwagger"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "trainingSwagger",
      "Config": [
        {
          "Name": "Training APIs",
          "Version": "v1",
          "Url": "http://localhost:5001/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "subscriptionSwagger",
      "Config": [
        {
          "Name": "Subscription APIs",
          "Version": "v1",
          "Url": "http://localhost:5002/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:52000"
  }
}